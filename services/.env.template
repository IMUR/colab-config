# Environment Variables Template for Colab Services
# Copy to .env and fill in actual values
# NEVER commit .env file!

# Admin Configuration
ADMIN_EMAIL=admin@example.com

# Semaphore Configuration
SEMAPHORE_ADMIN_PASSWORD=changeme
SEMAPHORE_ACCESS_KEY=changeme-generate-random-32-chars

# Prometheus Configuration
PROMETHEUS_RETENTION=15d

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=changeme
GRAFANA_URL=http://192.168.254.10:3001

# Node URLs (for service discovery)
CRTR_URL=http://192.168.254.10
PRTR_URL=http://192.168.254.20
DRTR_URL=http://192.168.254.30
